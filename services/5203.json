{
    "process": "backup_sa",
    "systemability": [
        {
            "name": 5203,
            "libpath": "libbackup_sa.z.so",
            "run-on-create": false,
            "auto-restart": true,
            "distributed": false,
            "dump_level": 1,
            "start-on-demand": {
                "commonevent": [
                    {
                        "name": "usual.event.BOOT_COMPLETED",
                        "conditions": [
                            {
                                "eventId": "param",
                                "name": "persist.backupservice.workstatus",
                                "value": "true"
                            }
                        ]
                    }
                ]
            }
        }
    ]
}