            "sandbox-path" : "/storage/Users/currentUser/Desktop/",
            "src-path" : "/mnt/hmdfs/<currentUserId>/account/merge_view/files/Desktop/"
        }, {
            "sandbox-path" : "/storage/Users/currentUser/",
            "src-path" : "/mnt/hmdfs/<currentUserId>/account/merge_view/files/Docs/"
